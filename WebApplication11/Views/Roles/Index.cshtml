@model IEnumerable<IdentityRole>


@{
    ViewData["Tital"] = "Roles";
}

<main id="main" class="main">

    <div class="pagetitle" style="width:fit-content">
    </div><!-- End Page Title -->

    <section class="section">



        <div class="card" style="width:100%;align-content:center">
            <div class="card-body" style="align-self:center;align-items:center;width:90%;align-content:center">
                <h5 class="card-title">Roles </h5>
                 
                <partial name="_Roleform" model="new RoleFormviewModel()" />
                <div>
                    <table class="table table-striped mt-4">
                        <thead>
                            <tr>
                                <th>ID</th>
                                <th>Role</th>
                            </tr>
                        </thead>

                        <tbody>
                            @if(Model is null)
                            {
                            }
                             @if(Model is not null)
                            {
                                
                            
                            @foreach (var role in Model)
                            {
                                <tr>
                                    <td>@role.Id</td>
                                    <td>@role.Name</td>
                                </tr>
                                }}
                        </tbody>
                    </table>
                </div>
  
            </div>
        </div>
  
    </section>

</main><!-- End #main -->

@section Scripts {
    <partial name="_ValidationScriptsPartial" />
}


 