@model WebApplication11.Models.HANDHELD

@{
    ViewData["Title"] = "Create";
}







<main id="main" class="main">

    <div class="pagetitle" style="width:fit-content">

        <nav>
            <ol class="breadcrumb">
                <li class="breadcrumb-item"><a href="index.html">Home</a></li>
                <li class="breadcrumb-item">DEVICES</li>
                <li class="breadcrumb-item active">Create</li>
            </ol>
        </nav>
    </div><!-- End Page Title -->

    <section class="section">
        @*<div class="row-cols-xxl-9" style="width:max-content">*@
           

                <div class="card" style="width:100%;align-content:center">
                    <div class="card-body" style="width:90%;align-self:center">
                        <h5 class="card-title">Create</h5>

                        <h4>HANDHELD</h4>
                        <hr />
                         
                            
                                <form asp-action="Create">
                                    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                                    <div class="form-group">
                                        <label asp-for="CODE" class="control-label"></label>
                                        <input asp-for="CODE" class="form-control" />
                                        <span asp-validation-for="CODE" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="NAME" class="control-label"></label>
                                        <input asp-for="NAME" class="form-control" />
                                        <span asp-validation-for="NAME" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="MODEL" class="control-label"></label>
                                        <input asp-for="MODEL" class="form-control" />
                                        <span asp-validation-for="MODEL" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="SERIAL_NUMBER" class="control-label"></label>
                                        <input asp-for="SERIAL_NUMBER" class="form-control" />
                                        <span asp-validation-for="SERIAL_NUMBER" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="Processor" class="control-label"></label>
                                        <input asp-for="Processor" class="form-control" />
                                        <span asp-validation-for="Processor" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="RAM_SIZE" class="control-label"></label>
                                        <input asp-for="RAM_SIZE" class="form-control" />
                                        <span asp-validation-for="RAM_SIZE" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="ANDROID_VERSION" class="control-label"></label>
                                        <input asp-for="ANDROID_VERSION" class="form-control" />
                                        <span asp-validation-for="ANDROID_VERSION" class="text-danger"></span>
                                    </div>
                                    <div class="form-group form-check">
                                        <label class="form-check-label">
                                            <input class="form-check-input" asp-for="Is_warehouse" /> @Html.DisplayNameFor(model => model.Is_warehouse)
                                        </label>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="RECIVED_DEVICE_DATE" class="control-label"></label>
                                        <input asp-for="RECIVED_DEVICE_DATE" class="form-control" />
                                        <span asp-validation-for="RECIVED_DEVICE_DATE" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="RE_RECIVED_DEVICE_DATE" class="control-label"></label>
                                        <input asp-for="RE_RECIVED_DEVICE_DATE" class="form-control" />
                                        <span asp-validation-for="RE_RECIVED_DEVICE_DATE" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label asp-for="NOTE" class="control-label"></label>
                                        <textarea asp-for="NOTE" class="form-control" ></textarea>
                                        <span asp-validation-for="NOTE" class="text-danger"></span>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">EMPLOYEE</label>
                                        <select asp-for="EMPLOYEE_ID" class="form-control" asp-items="ViewBag.EMPLOYEE_ID"></select>
                                    </div>
                                    <div class="form-group">
                                        <label class="control-label">WAREHOUSE STOCK</label>
                                        <select asp-for="WAREHOUSE_STOCK_ID" class="form-control" asp-items="ViewBag.WAREHOUSE_STOCK_ID"></select>
                                    </div>
                                    <div class="form-group">
                                        <input type="submit" value="Create" style="margin-left:6cm" class="btn btn-sim btn-outline-primary" />
                                <a asp-action="Index" class="btn btn-sim btn btn-outline-dark">Back to List</a>
                            </div> 
                                </form>
                           
                         



                    </div>
                </div>

             

        @*</div>*@
    </section>

</main><!-- End #main -->